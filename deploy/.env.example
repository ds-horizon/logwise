# LogWise Environment Configuration
# Copy this file to .env and fill in your values
# cp .env.example .env

# =============================================================================
# AWS Configuration (REQUIRED - Fill these in)
# =============================================================================
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here
AWS_SESSION_TOKEN=
# AWS_SESSION_TOKEN is optional, only needed for temporary credentials

# =============================================================================
# S3 Configuration (REQUIRED - Fill these in)
# =============================================================================
S3_BUCKET=your-bucket-name
S3_PREFIX=logs/

# =============================================================================
# Athena Configuration (REQUIRED - Fill these in)
# =============================================================================
S3_ATHENA_OUTPUT=s3://your-bucket/athena-output/
ATHENA_WORKGROUP=primary
ATHENA_CATALOG=AwsDataCatalog
ATHENA_DATABASE=logwise

# =============================================================================
# Kafka Configuration
# =============================================================================
KAFKA_BROKERS=kafka:9092
KAFKA_TOPIC=logs
KAFKA_CLUSTER_ID=9ZkYwXlQ2Tq8rBn5JcH0xA

# =============================================================================
# MySQL Database Configuration
# =============================================================================
MYSQL_DATABASE=myapp
MYSQL_USER=myapp
MYSQL_PASSWORD=myapp_pass
MYSQL_ROOT_PASSWORD=root_pass

# =============================================================================
# Spark Configuration
# =============================================================================
SPARK_MASTER_URL=spark://spark-master:7077
SPARK_STREAMING=true
SPARK_MASTER_UI_PORT=18080
SPARK_VERSION_MATCH=3.1.2
HADOOP_AWS_VERSION=3.2.0
AWS_SDK_VERSION=1.11.375
MAIN_CLASS=com.dream11.MainApplication
APP_ARGS=kafka.cluster.dns="kafka",kafka.cluster.name=central-log-management-v3,kafka.manager.host="http://kafka-manager:9000",kafka.maxRatePerPartition=4000,kafka.startingOffsets=latest,kafka.subscribePattern="logs.*",spark.master.host="http://spark-master:8080"

# =============================================================================
# Orchestrator Configuration
# =============================================================================
ORCH_PORT=8080

# =============================================================================
# Tenant Configuration
# =============================================================================
TENANT_VALUE=D11-Prod-AWS
